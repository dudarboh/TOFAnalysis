<marlin>
	<execute>
		<processor name="TOFAnalysis"/>
	</execute>

	<global>
		<parameter name="LCIOInputFiles">
            dummy.slcio
        </parameter>

		<parameter name="MaxRecordNumber" value="0" />
		<!-- tell Marlin to run the first n events, if n==0, then run all events -->
        <!-- <parameter name="SkipNEvents" value="10010" /> -->

        <parameter name="SupressCheck" value="false" />
		<parameter name="Verbosity" options="DEBUG0-4,MESSAGE0-4,WARNING0-4,ERROR0-4,SILENT">DEBUG</parameter>
		<!-- tell what debug information will be shown on the screen -->
		<parameter name="AllowToModifyEvent" value="false" />
		<!-- give Marlin right to modify events -->
	</global>

	<processor name="TOFAnalysis" type="myTOFAnalysis">
        <parameter name="output_filename"></parameter>
	</processor>

	<constants>
	</constants>

</marlin>
